-> 1D LinkedList
class Node{
    Public:
    int data; 
    Node* next;
    Public:
    Node (int data1){
        data=data1; 
        next=nullptr;  
    }
};

-> Doubly Linked List
class Node {
public:
    int data; 
    Node* next;    
    Node* back;    
    Node(int data1) {
        data = data1;
        next = nullptr; 
        back = nullptr; 
    }
};
